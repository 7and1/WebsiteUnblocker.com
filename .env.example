# Environment Variables Template
# Copy this file to .env.local for local development (DO NOT commit .env.local)

# Cloudflare
CLOUDFLARE_ACCOUNT_ID=your-account-id-here
CLOUDFLARE_API_TOKEN=your-api-token-here

# D1 Database (optional, for local)
D1_DATABASE_ID=your-d1-database-id-here

# Payload CMS
PAYLOAD_SECRET=generate-with-openssl-rand-hex-32

# Application
NODE_ENV=production
NEXT_PUBLIC_SERVER_URL=https://websiteunblocker.com
